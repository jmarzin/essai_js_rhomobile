<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @model001.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @model001.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="model001[Titre]" class="fieldLabel">Titre</label>
            <input type="text" id="model001[Titre]" name="model001[Titre]" <%= placeholder("Titre") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="model001[Nom]" class="fieldLabel">Nom</label>
            <input type="text" id="model001[Nom]" name="model001[Nom]" <%= placeholder("Nom") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
