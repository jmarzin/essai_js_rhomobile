<div data-role="page">

	<div data-role="header" data-position="inline">
    <h1>Model001s</h1>
    <a href="<%= Rho::Application.startURI %>" class="ui-btn-left" data-icon="home" data-direction="reverse" <%= "data-ajax='false'" if is_bb6 %>>
      Home
    </a>
    <a href="<%= url_for :action => :new %>" class="ui-btn-right" data-icon="plus">
      New
    </a>
	</div>

  <div data-role="content">
    <ul data-role="listview">
      <% @model001s.each do |model001| %>
      
          <li>
            <a href="<%= url_for :action => :show, :id => model001.object %>">
              <%= model001.Titre %>
            </a>
          </li>
      
      <% end %>
    </ul>
  </div>
  
  <button id='bouton' type="button">Click Me!</button> 

  <script type="text/javascript">
      $('#bouton').click(function (event) {
        helloWorld();
      });
  </script> 
</div>
